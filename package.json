{
  "name": "canxjs",
  "version": "1.6.4",
  "description": "Ultra-fast async-first MVC backend framework for Bun runtime",
  "type": "module",
  "main": "./dist/src/index.js",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "types": "./dist/src/index.d.ts"
    },
    "./router": {
      "import": "./dist/src/core/Router.js",
      "types": "./dist/src/core/Router.d.ts"
    },
    "./controller": {
      "import": "./dist/src/mvc/Controller.js",
      "types": "./dist/src/mvc/Controller.d.ts"
    },
    "./model": {
      "import": "./dist/src/mvc/Model.js",
      "types": "./dist/src/mvc/Model.d.ts"
    },
    "./view": {
      "import": "./dist/src/mvc/View.js",
      "types": "./dist/src/mvc/View.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/src/mvc/jsx-runtime.js",
      "types": "./dist/src/mvc/jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/src/mvc/jsx-runtime.js",
      "types": "./dist/src/mvc/jsx-runtime.d.ts"
    },
    "./cli": {
      "import": "./dist/src/cli/index.js",
      "types": "./dist/src/cli/index.d.ts"
    },
    "./microservices": {
      "import": "./dist/src/microservices/index.js",
      "types": "./dist/src/microservices/index.d.ts"
    },
    "./cqrs": {
      "import": "./dist/src/cqrs/index.js",
      "types": "./dist/src/cqrs/index.d.ts"
    },
    "./testing": {
      "import": "./dist/src/testing/index.js",
      "types": "./dist/src/testing/index.d.ts"
    },
    "./search": {
      "import": "./dist/src/search/index.js",
      "types": "./dist/src/search/index.d.ts"
    },
    "./storage": {
      "import": "./dist/src/storage/index.js",
      "types": "./dist/src/storage/index.d.ts"
    },
    "./vite": {
      "import": "./dist/src/vite/index.js",
      "types": "./dist/src/vite/index.d.ts"
    },
    "./inertia": {
      "import": "./dist/src/inertia/index.js",
      "types": "./dist/src/inertia/index.d.ts"
    },
    "./graphql": {
      "import": "./dist/src/graphql/index.js",
      "types": "./dist/src/graphql/index.d.ts"
    }
  },
  "bin": {
    "canx": "./dist/bin/canx.js"
  },
  "files": [
    "dist",
    "cli",
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "start": "bun run src/main.ts",
    "dev": "bun --watch src/main.ts",
    "test": "bun test",
    "test:integration": "bun test ./tests/integration",
    "benchmark": "bun run ./benchmarks/run.ts",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "bun",
    "framework",
    "mvc",
    "backend",
    "async",
    "fast",
    "typescript",
    "jsx",
    "orm",
    "router",
    "rest-api",
    "web-framework",
    "nodejs-alternative",
    "server",
    "http",
    "middleware",
    "controller",
    "model",
    "view"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "bun": ">=1.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.23",
    "mysql2": "^3.9.0",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.16.0",
    "@types/reflect-metadata": "^0.1.0",
    "pg": "^8.11.0",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chandafa/canx.JS.git"
  },
  "bugs": {
    "url": "https://github.com/chandafa/canx.JS/issues"
  },
  "homepage": "https://github.com/chandafa/canx.JS#readme",
  "publishConfig": {
    "access": "public"
  }
}
